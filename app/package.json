    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@sveltejs/adapter-vercel": "^3.0.0",
    "@types/react": "^18.0.26",
    "@urql/core": "^4.0.0",
    "autoprefixer": "^10.2.4",
    "graphql": "^16.0.0",
    "html-minifier": "^4.0.0",
    "nprogress": "^0.2.0",
    "playwright": "^1.31.0",
    "rehype-autolink-headings": "^6.0.0",
    "rehype-code-titles": "^1.1.0",
    "rehype-slug": "^5.0.0",
    "rehype-stringify": "^9.0.0",
    "remark-contentful-image": "^0.0.0",
    "remark-gfm": "^3.0.0",
    "remark-hint": "^1.0.9",
    "remark-link-card": "^0.0.0",
    "remark-parse": "^10.0.0",
    "remark-rehype": "^10.1.0",
    "satori": "^0.10.0",
