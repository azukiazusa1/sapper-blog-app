    "@storybook/svelte": "^7.0.0-beta.50",
    "@storybook/sveltekit": "^7.0.0-beta.50",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@sveltejs/kit": "^1.5.6",
    "@tsconfig/svelte": "^4.0.0",
    "@types/html-minifier": "^4.0.2",
