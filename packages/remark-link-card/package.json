{
  "name": "remark-link-card",
  "version": "0.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix ./src",
    "format:check": "prettier --check --plugin-search-dir=. ./src",
    "format": "prettier --write --plugin-search-dir=. ./src",
    "dev": "tsc -w",
    "build": "tsc",
    "prebuild": "rimraf build",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-custom": "^0.0.0",
    "msw": "1.0.1",
    "node-fetch": "^3.3.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.2",
    "rimraf": "^5.0.0",
    "tsconfig": "^0.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "open-graph-scraper": "^5.0.1",
    "sanitize-html": "^2.10.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2"
  }
}
