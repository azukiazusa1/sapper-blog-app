{
  "name": "rehype-alert",
  "version": "0.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix ./src",
    "format:check": "prettier --check --config-path=../../.prettierrc ./src",
    "format": "prettier --write --config-path=../../.prettierrc ./src",
    "dev": "tsc -w",
    "prebuild": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update": "vitest run --update",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-custom": "^0.0.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.1",
    "rimraf": "^5.0.0",
    "tsconfig": "^0.0.0",
    "typescript": "^5.3.3",
    "unist-util-visit": "^4.1.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "unified": "^10.1.2"
  }
}
