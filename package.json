  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "mock": "NODE_MOCK=true vite dev",
    "postbuild": "npx svelte-sitemap --domain $VITE_BASE_URL --out-dir ./.vercel/output/static",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "serve": "npx serve ./.vercel/output/static",
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint ./src",
    "lint:eslint:fix": "eslint --fix ./src",
    "format:check": "prettier --check --plugin-search-dir=. ./src",
    "format": "prettier --write --plugin-search-dir=. ./src",
    "gen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "test": "playwright test",
    "test:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.29.1-focal /bin/bash -c 'npm test -- -u'",
    "typecheck": "tsc --noEmit && svelte-check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "GH_PAGE=true storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"npx wait-on http://127.0.0.1:6006 && npx test-storybook --url http://127.0.0.1:6006\""
  },
